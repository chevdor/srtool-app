{
	"name": "srtool-app",
	"version": "0.9.0",
	"description": "Srtool App",
	"main": "./build/main.js",
	"homepage": "https://gitlab.com/chevdor/srtool-app",
	"keywords": [
		"polkadot",
		"kusama",
		"substrate",
		"runtime",
		"wasm"
	],
	"author": "chevdor <chevdor@gmail.com>",
	"license": "MIT",
	"scripts": {
		"dev:electron": "NODE_ENV=development webpack --config webpack.electron.config.js --mode development && electron .",
		"dev:react": "NODE_ENV=development webpack serve --config webpack.react.config.js --mode development",
		"build:electron": "NODE_ENV=production webpack --config webpack.electron.config.js --mode production",
		"build:react": "NODE_ENV=production webpack --config webpack.react.config.js --mode production",
		"build": "npm run build:electron && npm run build:react",
		"pack": "electron-builder --dir",
		"dist": "electron-builder",
		"dist:mac": "npm run build && electron-builder build --mac",
		"dist:linux": "npm run build && electron-builder build --linux",
		"dist:win": "npm run build && electron-builder build --win",
		"dist:all": "npm run build && electron-builder build --mac --linux --win",
		"ci:mac": "electron-builder build --mac",
		"ci:linux": "electron-builder build --linux",
		"ci:win": "electron-builder build --win",
		"test": "mocha -r ts-node/register test/**/*.spec.ts",
		"clean": "rm -rf node_modules builds build/{js,main.*,index.html} dist *.log",
		"postinstall": "electron-builder install-app-deps",
		"ci:test:mac": "gitlab-runner exec docker pack:mac",
		"lint": "eslint --ext .ts,.tsx .",
		"check": "tsc --noEmit --pretty"
	},
	"devDependencies": {
		"@babel/core": "7.13.14",
		"@babel/preset-env": "7.13.12",
		"@babel/preset-react": "7.13.13",
		"@babel/preset-typescript": "7.13.0",
		"@types/chai": "4.2.16",
		"@types/cors": "2.8.10",
		"@types/dockerode": "3.2.3",
		"@types/mkdirp": "^1.0.1",
		"@types/mocha": "8.2.2",
		"@types/node": "14.14.37",
		"@types/rimraf": "^3.0.0",
		"@types/semver": "^7.3.4",
		"@types/unzipper": "^0.10.3",
		"@typescript-eslint/eslint-plugin": "4.20.0",
		"@typescript-eslint/parser": "4.20.0",
		"babel-core": "^6.26.3",
		"babel-loader": "^8.2.2",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-polyfill": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babel-regenerator-runtime": "^6.5.0",
		"chai": "4.3.4",
		"css-loader": "5.2.0",
		"electron": "11.4.2",
		"electron-builder": "22.10.5",
		"eslint": "7.23.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-promise": "^4.3.1",
		"eslint-plugin-standard": "^5.0.0",
		"html-webpack-plugin": "5.3.1",
		"mini-css-extract-plugin": "1.4.0",
		"mocha": "8.3.2",
		"prettier": "^2.2.1",
		"style-loader": "^2.0.0",
		"ts-node": "9.1.1",
		"typescript": "4.2.3",
		"webpack": "5.30.0",
		"webpack-cli": "4.6.0",
		"webpack-dev-server": "^3.11.2"
	},
	"dependencies": {
		"@material-ui/core": "^4.11.3",
		"@material-ui/icons": "^4.11.2",
		"@material-ui/lab": "^4.0.0-alpha.57",
		"@types/react": "17.0.3",
		"@types/react-dom": "17.0.3",
		"check-disk-space": "^2.1.0",
		"dockerode": "^3.2.1",
		"electron-is": "^3.0.0",
		"electron-store": "^7.0.2",
		"javascript-terminal": "^1.1.1",
		"node-fetch": "^2.6.1",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-jss": "10.6.0",
		"react-terminal-component": "^1.5.0",
		"semver": "7.3.5",
		"styled-components": "5.2.3",
		"unzipper": "^0.10.11"
	}
}
